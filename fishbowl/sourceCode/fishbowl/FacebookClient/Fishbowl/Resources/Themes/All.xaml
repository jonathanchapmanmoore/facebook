  </LinearGradientBrush>

  <ControlTemplate x:Key="StandardButtonChromeNormalControlTemplate" TargetType="{x:Type Control}">
    <Grid>
      <Rectangle RadiusX="5" RadiusY="5" Margin="-.25,0,-.25,-1" Fill="#22000000"/>
      <Rectangle RadiusX="5" RadiusY="5" Margin="-.5,-.25,-.5,-2" Fill="#11000000"/>
      <Rectangle RadiusX="4" RadiusY="4" StrokeThickness="1" Stroke="White" Fill="{DynamicResource StandardButtonChromeNormalBrush}" />
    </Grid>
  </ControlTemplate>

  <ControlTemplate x:Key="StandardButtonChromePressedControlTemplate" TargetType="{x:Type Control}">
    <Grid>
      <Rectangle RadiusX="4" RadiusY="4" Stroke="White" StrokeThickness="1">
        <Rectangle.Fill>
          <LinearGradientBrush StartPoint="0,.5" EndPoint="10,.5" MappingMode="Absolute">
            <GradientStop Color="#A1B3DB" Offset="0"/>
            <GradientStop Color="#B9CEFB" Offset=".3"/>
          </LinearGradientBrush>
        </Rectangle.Fill>
      </Rectangle>
      <Rectangle RadiusX="4" RadiusY="4" Stroke="White" StrokeThickness="1" Fill="{DynamicResource StandardButtonChromePressedBrush}"/>
    </Grid>
  </ControlTemplate>

  <ControlTemplate x:Key="StandardButtonChromeHoverControlTemplate" TargetType="{x:Type Control}">
    <Grid>
      <Rectangle RadiusX="5" RadiusY="5" Margin="-.25,0,-.25,-1" Fill="#22000000"/>
      <Rectangle RadiusX="5" RadiusY="5" Margin="-.5,-.25,-.5,-2" Fill="#11000000"/>
      <Rectangle RadiusX="4" RadiusY="4" Stroke="White" StrokeThickness="1" Fill="{DynamicResource StandardButtonChromeHoverBrush}"/>
    </Grid>
  </ControlTemplate>

  <ControlTemplate x:Key="StandardPhotoChromeContentControlTemplate" TargetType="{x:Type ContentControl}">
    <Grid Margin="{TemplateBinding Margin}">
      <Rectangle Margin="2,4,-2,-4" Fill="#11000000"/>
      <Rectangle Margin="4,2,-4,-2" Fill="#11000000"/>
      <Rectangle Margin="-2" Fill="#11000000"/>
      <Rectangle Fill="White" />
      <ContentPresenter x:Name="Content"
          Content="{TemplateBinding Content}" 
          Margin="{TemplateBinding Padding}" 
          HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
          VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
    </Grid>
  </ControlTemplate>

  <ControlTemplate x:Key="StandardTextBoxChromeControlTemplate" TargetType="{x:Type Control}">
    <Grid>
            <Rectangle RadiusX="4" RadiusY="4" StrokeThickness="1">
                <Rectangle.Stroke>
                    <LinearGradientBrush StartPoint="0,.5" EndPoint="3,.5" MappingMode="Absolute">
                        <GradientStop Color="#979797" Offset="0"/>
                        <GradientStop Color="#00979797" Offset="1"/>
                    </LinearGradientBrush>
                </Rectangle.Stroke>
                <Rectangle.Fill>
                    <LinearGradientBrush StartPoint="0,.5" EndPoint="5,.5" MappingMode="Absolute">
                        <GradientStop Color="#DBDBDB" Offset="0"/>
                        <GradientStop Color="Transparent" Offset="1"/>
                    </LinearGradientBrush>
                </Rectangle.Fill>
            </Rectangle>
            <Rectangle RadiusX="4" RadiusY="4" StrokeThickness="1">
                <Rectangle.Stroke>
                    <LinearGradientBrush StartPoint=".5,0" EndPoint=".5,3" MappingMode="Absolute">
                        <GradientStop Color="#979797" Offset="0"/>
                        <GradientStop Color="#00979797" Offset="1"/>
                    </LinearGradientBrush>
                </Rectangle.Stroke>
                <Rectangle.Fill>
                    <LinearGradientBrush StartPoint=".5,0" EndPoint=".5,5" MappingMode="Absolute">
                        <GradientStop Color="#DBDBDB" Offset="0"/>
                        <GradientStop Color="Transparent" Offset="1"/>
                    </LinearGradientBrush>
                </Rectangle.Fill>
            </Rectangle>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="StandardRoundButtonChromeNormalControlTemplate" TargetType="{x:Type Control}">
        <Grid>
            <Rectangle RadiusX="5" RadiusY="16" Margin="-.25,0,-.25,-1" Fill="#22000000"/>
            <Rectangle RadiusX="5" RadiusY="16" Margin="-.5,-.25,-.5,-2" Fill="#11000000"/>
            <Rectangle RadiusX="4" RadiusY="15" StrokeThickness="1" Stroke="White" >
                <Rectangle.Fill>
                    <LinearGradientBrush StartPoint=".5,0" EndPoint=".5,1">
                        <GradientStop Color="#EEF3FE" Offset="0"/>
                        <GradientStop Color="#C1D3FB" Offset="1"/>
                    </LinearGradientBrush>
                </Rectangle.Fill>
            </Rectangle>
        </Grid>
    </ControlTemplate>

    <ControlTemplate TargetType="{x:Type ToggleButton}" x:Key="StandardToggleButtonControlTemplate">
        <Grid x:Name="Root" Background="Transparent" SnapsToDevicePixels="True">
            <Control x:Name="up" Template="{StaticResource StandardButtonChromeNormalControlTemplate}"/>
            <Control x:Name="hover" Opacity="0" Template="{StaticResource StandardButtonChromeHoverControlTemplate}"/>
            <Control x:Name="down" Opacity="0" Template="{StaticResource StandardButtonChromePressedControlTemplate}"/>
            <ContentPresenter x:Name="Content"
          Content="{TemplateBinding Content}" 
          Margin="{TemplateBinding Padding}" 
          HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
          VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Trigger.ExitActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation To="1" Duration="0:0:0.3" Storyboard.TargetName="Root" Storyboard.TargetProperty="Opacity" />
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.ExitActions>
                <Trigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation To=".6" Duration="0:0:0.4" Storyboard.TargetName="Root" Storyboard.TargetProperty="Opacity" />
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.EnterActions>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Trigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation To="1" Duration="0:0:0.3" Storyboard.TargetName="hover" Storyboard.TargetProperty="Opacity" />
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.EnterActions>
                <Trigger.ExitActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation To="0" Duration="0:0:0.4" Storyboard.TargetName="hover" Storyboard.TargetProperty="Opacity" />
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.ExitActions>
            </Trigger>
            <Trigger Property="IsChecked" Value="True">
                <Trigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation To="1" Duration="0:0:0.3" Storyboard.TargetName="down" Storyboard.TargetProperty="Opacity" />
                            <!--<DoubleAnimation To="0" Duration="0:0:0.2" Storyboard.TargetName="hover" Storyboard.TargetProperty="Opacity" />-->
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.EnterActions>
                <Trigger.ExitActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation To="0" Duration="0:0:0.4" Storyboard.TargetName="down" Storyboard.TargetProperty="Opacity" />
                            <!--<DoubleAnimation To="1" Duration="0:0:0.3" Storyboard.TargetName="hover" Storyboard.TargetProperty="Opacity" />-->
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.ExitActions>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Trigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation To="1" Duration="0:0:0.3" Storyboard.TargetName="down" Storyboard.TargetProperty="Opacity" />
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.EnterActions>
                <Trigger.ExitActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation To="0" Duration="0:0:0.4" Storyboard.TargetName="down" Storyboard.TargetProperty="Opacity" />
                        </Storyboard>
                    </BeginStoryboard>
                </Trigger.ExitActions>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

