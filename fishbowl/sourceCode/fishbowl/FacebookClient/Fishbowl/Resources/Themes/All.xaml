  <LinearGradientBrush x:Key="StandardButtonChromeNormalBrush" StartPoint=".5,0" EndPoint=".5,1">
    <GradientStop Color="#EEF3FE" Offset="0"/>
    <GradientStop Color="#C1D3FB" Offset="1"/>
  </LinearGradientBrush>

  <LinearGradientBrush x:Key="StandardButtonChromePressedBrush" StartPoint=".5,0" EndPoint=".5,1">
    <GradientStop Color="#A1B3DB" Offset="0"/>
    <GradientStop Color="#00B9CEFB" Offset=".3"/>
  </LinearGradientBrush>

